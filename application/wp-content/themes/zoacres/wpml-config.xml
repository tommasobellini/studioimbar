	<shortcodes>
        <shortcode>
            <tag>zoacres_vc_blog</tag>
			<attributes>
                <attribute>more_text</attribute>
            </attributes>
        </shortcode>
        <shortcode>
            <tag>zoacres_vc_circle_progress</tag>
